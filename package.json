    "gulp-concat": "^2.6.1",
    "gulp-include": "^2.3.1",
    "gulp-order": "^1.1.1",
    "gulp-postcss": "^6.2.0",
    "gulp-sass": "^2.3.2",
    "gulp-sass-glob": "^1.0.6",
    "gulp-uglify": "^2.0.0",
    "gulp-uglifycss": "^1.0.6",
    "gulp-util": "^3.0.8",
    "markdown-toc": "^1.2.0",
    "react-scripts": "1.0.11",
    "truffle": "^3.4.9"
  },
  "dependencies": {
    "clipboard": "^1.7.1",
    "immutability-helper": "^2.5.0",
    "install": "^0.10.1",
    "jquery": "^3.2.1",
    "jszip": "^3.1.4",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.3",
    "npm": "^5.3.0",
    "solidity-flattener": "file:submodules/solidity-flattener",
    "path": "^0.12.7",
    "qrcode.react": "^0.7.2",
    "react": "^15.6.1",
    "react-alert": "^2.4.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-countdown-clock": "^2.0.0",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "solc": "^0.4.14",
